{
  "name": "twilio-cli",
  "description": "unleash the power of Twilio from your command prompt",
  "version": "1.1.2",
  "author": "Twilio @twilio",
  "bin": {
    "twilio": "./bin/run"
  },
  "bugs": "https://github.com/twilio/twilio-cli/issues",
  "dependencies": {
    "@oclif/command": "^1.5.14",
    "@oclif/config": "^1.13.0",
    "@oclif/plugin-autocomplete": "^0.1.1",
    "@oclif/plugin-help": "^2.2.0",
    "@oclif/plugin-plugins": "^1.7.8",
    "@twilio/cli-core": "^1.2.0",
    "chalk": "^2.4.2",
    "ngrok": "^3.1.1",
    "twilio": "^3.31.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.0",
    "@oclif/test": "^1.2.4",
    "@twilio/cli-test": "^1.0.0",
    "chai": "^4.2.0",
    "eslint": "^4.19.1",
    "eslint-config-oclif": "^1.5.1",
    "eslint-plugin-mocha": "^5.2.1",
    "globby": "^8.0.2",
    "mocha": "^5.2.0",
    "nock": "^10.0.6",
    "nyc": "^14.1.1",
    "rimraf": "^2.6.3",
    "sinon": "^6.3.5",
    "tmp": "0.0.33"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src"
  ],
  "homepage": "https://github.com/twilio/twilio-cli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "src/index.js",
  "oclif": {
    "commands": "./src/commands",
    "bin": "twilio",
    "scope": "twilio",
    "plugins": [
      "@oclif/plugin-autocomplete",
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topics": {
      "projects": {
        "description": "manage credentials for Twilio projects"
      }
    },
    "hooks": {
      "init": "./src/hooks/init/twilio-api",
      "post-api-plugin": "./src/hooks/post-api-plugin/plugin-verification"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/twilio/twilio-cli.git"
  },
  "scripts": {
    "postpack": "rimraf oclif.manifest.json npm-shrinkwrap.json",
    "posttest": "eslint --ignore-path .gitignore . && npm audit",
    "prepack": "oclif-dev manifest && oclif-dev readme && npm shrinkwrap && git checkout -- package-lock.json",
    "test": "nyc --check-coverage --lines 90 --reporter=html --reporter=text mocha --forbid-only \"test/**/*.test.js\"",
    "version": "oclif-dev readme && git add README.md",
    "postinstall": "node welcome.js"
  }
}
