{
  "name": "twilio-cli",
  "description": "unleash the power of Twilio from your command prompt",
  "version": "2.4.0",
  "author": "Twilio @twilio",
  "bin": {
    "twilio": "./bin/run"
  },
  "bugs": "https://github.com/twilio/twilio-cli/issues",
  "dependencies": {
    "@oclif/color": "^0.1.2",
    "@oclif/command": "^1.7.0",
    "@oclif/config": "^1.16.0",
    "@oclif/plugin-autocomplete": "^0.2.0",
    "@oclif/plugin-help": "^3.1.0",
    "@oclif/plugin-plugins": "^1.8.3",
    "@oclif/plugin-warn-if-update-available": "^1.7.0",
    "@sendgrid/mail": "^7.2.1",
    "@twilio/cli-core": "^5.6.0",
    "chalk": "^4.1.0",
    "file-type": "^14.6.2",
    "inquirer": "^7.3.0",
    "twilio": "^3.47.0"
  },
  "optionalDependencies": {
    "ngrok": "^3.2.7"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@oclif/test": "^1.2.6",
    "@twilio/cli-test": "^2.1.0",
    "aws-sdk": "^2.709.0",
    "chai": "^4.2.0",
    "eslint": "^7.3.1",
    "eslint-config-oclif": "^3.1.0",
    "eslint-plugin-mocha": "^7.0.1",
    "globby": "^11.0.1",
    "mocha": "^8.0.1",
    "nock": "^13.0.2",
    "nyc": "^15.1.0",
    "proxyquire": "^2.1.3",
    "sinon": "^9.0.2",
    "tmp": "^0.2.1"
  },
  "engines": {
    "node": ">=10.12.0"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src",
    "/welcome.js"
  ],
  "homepage": "https://github.com/twilio/twilio-cli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "src/index.js",
  "oclif": {
    "commands": "./src/commands",
    "bin": "twilio",
    "scope": "twilio",
    "plugins": [
      "@oclif/plugin-autocomplete",
      "@oclif/plugin-help",
      "@oclif/plugin-plugins",
      "@oclif/plugin-warn-if-update-available"
    ],
    "warn-if-update-available": {
      "timeoutInDays": 1,
      "message": "<%= config.name %> update available from <%= chalk.greenBright(config.version) %> to <%= chalk.greenBright(latest) %>.",
      "registry": "https://registry.npmjs.org"
    },
    "topics": {
      "phone-numbers": {
        "description": "manage Twilio phone numbers"
      },
      "profiles": {
        "description": "manage credentials for Twilio profiles"
      }
    },
    "hooks": {
      "init": [
        "./src/hooks/init/twilio-api",
        "./src/hooks/init/buy-phone-number",
        "./src/hooks/init/plugin-verification"
      ],
      "plugins:preinstall": [
        "./src/hooks/plugin-install"
      ],
      "command_not_found": [
        "./src/hooks/command-not-found"
      ]
    },
    "macos": {
      "identifier": "com.twilio.cli"
    },
    "update": {
      "s3": {
        "bucket": "twilio-cli-prod"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/twilio/twilio-cli.git"
  },
  "scripts": {
    "postpack": "rm -f oclif.manifest.json npm-shrinkwrap.json",
    "posttest": "eslint --ignore-path .gitignore . && npm audit --audit-level=moderate",
    "prepack": "oclif-dev manifest && oclif-dev readme && npm shrinkwrap && git checkout -- package-lock.json",
    "test": "nyc --check-coverage --lines 90 --reporter=html --reporter=text mocha --forbid-only \"test/**/*.test.js\"",
    "version": "oclif-dev readme && git add README.md",
    "postinstall": "node welcome.js"
  }
}
